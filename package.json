{
  "private": true,
  "name": "pushkins-prefers",
  "description": "It's a zine that we are making",
  "license": "UNLICENSED",
  "author": "Greg Wood",
  "version": "1.0.0",
  "scripts": {
    "browserslist": "browserslist",
    "build": "gatsby build --prefix-paths",
    "develop": "netlify dev -c 'gatsby develop'",
    "format": "prettier --write 'src/**/*.js'",
    "heroku-postbuild": "gatsby build --prefix-paths",
    "lint": "eslint src/. --ext .js; exit 0;",
    "lint-fix": "eslint src/. --ext .js --fix; exit 0;",
    "lint:staged": "lint-staged",
    "start": "gatsby serve",
    "test": "jest --notify --runInBand --watchAll",
    "test-coverage": "jest --coverage --runInBand"
  },
  "dependencies": {
    "autoprefixer": "9.5.1",
    "browserslist": "4.5.6",
    "classnames": "2.2.6",
    "dotenv": "8.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-preset-gatsby": "0.1.11",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-environment-enzyme": "7.0.2",
    "jest-environment-jsdom": "24.7.1",
    "jest-environment-jsdom-global": "1.2.0",
    "jest-enzyme": "7.0.2",
    "jest-fetch-mock": "2.1.2",
    "jest-plugin-context": "2.9.0",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "prettier-package-json": "2.1.0",
    "prettier-stylelint": "0.4.2",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "7.0.1",
    "sass": "1.20.1",
    "stylelint": "10.0.1",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-order": "3.0.0",
    "stylelint-scss": "3.6.1"
  },
  "engines": {
    "node": "10.15.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --syntax=scss",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
